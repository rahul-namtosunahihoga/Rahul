#include <stdio.h>
main() 
{ int n;
  scanf("%d",&n);
  int a,i,c,j,x,*m;
  int arr[n];
  m=arr;
  x= get_them(n,arr);
 	printf("%d",x);
	int sum[100];
	printf("%d",arr[0]);
 	for(i=10;i<x;i++)
 {	
 	for(j=0;j<i;j++)
 	c=arr[i]+arr[j];
	 sum[i]=c;
 }

	for(i=0;i<n;i++)
	{  	
 		if(n==sum[i])
    	printf("Yes");
    }
}

int get_them(int n, int *m)
{	
    int i,j,x=0,k;
	
	for(i=3;i<n;i++)
    {
        for(j=2;j<i;)
        {	
            k=i%j;j++;
  	        if(k==0)
 	        break;
       
        }
  
  		if(j==i)
	   {
		*(m+i)=i;
	    x++;
	   }
    }
 	 return x;
	

}
